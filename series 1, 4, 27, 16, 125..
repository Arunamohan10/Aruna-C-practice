#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    int n,i,one,two;
    cin >> n;
    for(i=1;i<=n;i++)
    {
        if(i%2==0)
        {
            one=i*i;
            cout << one << " ";
        }
        else
        {
            two=i*(i*i);
            cout << two << " ";
        }
    }
}
